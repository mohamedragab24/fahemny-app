rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // WARNING: These rules are overly permissive and are intended for immediate debugging.
      // They allow any authenticated user to read and write all data.
      // This MUST be secured before going to production.
      allow read, write: if request.auth != null;
    }
  }
}
