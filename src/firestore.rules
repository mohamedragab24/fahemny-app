rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are temporarily open for debugging and are not secure.
    // They allow any authenticated user to read and write all data.
    // TODO: Re-implement secure, granular rules before production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
